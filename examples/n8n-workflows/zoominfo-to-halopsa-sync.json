    {
      "parameters": {
        "jsCode": "// Log successful creation/update for monitoring\nconst leadData = $input.first().json;\n\nconsole.log('ZoomInfo Lead Processing:', {\n  action: leadData.id ? 'created' : 'updated',\n  email: leadData.emailaddress,\n  company: leadData.client_name,\n  quality_score: leadData.data_quality_score,\n  timestamp: new Date().toISOString()\n});\n\n// Prepare success response\nreturn [{\n  status: 'success',\n  action: leadData.id ? 'created' : 'updated',\n  lead_id: leadData.id,\n  email: leadData.emailaddress,\n  company: leadData.client_name,\n  quality_score: leadData.data_quality_score\n}];"
      },
