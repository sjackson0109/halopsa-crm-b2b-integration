    {
      "parameters": {
        "jsCode": "// Transform ZoomInfo contact data to HaloPSA Lead format\nconst contact = $input.first().json.data.contact;\n\n// Calculate data quality score\nconst calculateDataQualityScore = (contact) => {\n  let score = 0;\n  \n  // Email validation (30 points)\n  if (contact.email && contact.email.includes('@')) {\n    score += 30;\n  }\n  \n  // Phone number (25 points)\n  if (contact.directPhone || contact.companyPhone) {\n    score += 25;\n  }\n  \n  // Complete name (20 points)\n  if (contact.firstName && contact.lastName) {\n    score += 20;\n  }\n  \n  // Company data (15 points)\n  if (contact.companyName && contact.companyWebsite && contact.industry) {\n    score += 15;\n  }\n  \n  // Social media presence (10 points)\n  if (contact.linkedInUrl) {\n    score += 10;\n  }\n  \n  return score;\n};\n\nconst dataQualityScore = calculateDataQualityScore(contact);\n\n// Skip if data quality is too low\nif (dataQualityScore < 50) {\n  return [{\n    skip: true,\n    reason: 'Data quality score too low',\n    score: dataQualityScore\n  }];\n}\n\n// Transform to HaloPSA Lead format\nconst haloLead = {\n  // Standard ticket fields\n  summary: `${contact.firstName} ${contact.lastName} - ${contact.companyName}`,\n  details: `Lead imported from ZoomInfo enrichment. Data Quality Score: ${dataQualityScore}/100`,\n  user_name: `${contact.firstName} ${contact.lastName}`,\n  reportedby: contact.email,\n  category_1: \"Lead\",\n  category_2: \"ZoomInfo\",\n  status_id: 1, // New Lead\n  priority_id: 3, // Medium\n  \n  // Contact information\n  firstname: contact.firstName,\n  surname: contact.lastName,\n  emailaddress: contact.email,\n  jobtitle: contact.jobTitle,\n  phonenumber: contact.directPhone || contact.companyPhone,\n  \n  // Company information\n  client_name: contact.companyName,\n  client_website: contact.companyWebsite,\n  client_phone: contact.companyPhone,\n  \n  // Custom fields\n  customfields: [\n    {\n      id: 101, // Lead Source\n      value: \"ZoomInfo\"\n    },\n    {\n      id: 102, // Services Offered\n      value: `${contact.industry} | ${contact.companyDescription || 'Industry solution services'}`\n    },\n    {\n      id: 103, // Growth Signals\n      value: `Revenue: ${contact.revenue} | Employees: ${contact.employeeCount} | Level: ${contact.managementLevel} | Dept: ${contact.department}`\n    },\n    {\n      id: 104, // Project Pipelines\n      value: contact.technologies ? contact.technologies.join(', ') : 'Technology modernization'\n    },\n    {\n      id: 105, // Do Not Contact\n      value: false\n    }\n  ],\n  \n  // Additional metadata\n  notes: `Enriched via ZoomInfo on ${new Date().toISOString()}\\n` +\n         `Management Level: ${contact.managementLevel}\\n` +\n         `Department: ${contact.department}\\n` +\n         `LinkedIn: ${contact.linkedInUrl || 'Not available'}\\n` +\n         `Data Quality Score: ${dataQualityScore}/100`,\n  \n  // ZoomInfo tracking\n  zoominfo_person_id: contact.personId,\n  zoominfo_company_id: contact.companyId,\n  data_quality_score: dataQualityScore\n};\n\nreturn [haloLead];"
      },
